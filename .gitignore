# --------------------------------------------------------------------------------
# Maven 构建产物
# --------------------------------------------------------------------------------
target/
!.mvn/wrapper/maven-wrapper.jar
!**/src/main/**/target/
!**/src/test/**/target/

# --------------------------------------------------------------------------------
